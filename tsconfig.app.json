/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@app/core/*": [
        "src/app/core/*"
      ],
      "@app/shared/*": [
        "src/app/shared/*"
      ],
      "@app/pages/*": [
        "src/app/pages/*"
      ],
      "@app/pages/home/*": [
        "src/app/pages/home/*"
      ],
      "@app/pages/home/components/*": [
        "src/app/pages/home/components/*"
      ],
      "@app/pages/home/containers/*": [
        "src/app/pages/home/containers/*"
      ],
      "@app/pages/login/*": [
        "src/app/pages/login/*"
      ],
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
